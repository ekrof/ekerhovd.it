    content:attr(data-domain) &#34;\0040&#34; attr(data-user);
    unicode-bidi:bidi-override;
    direction:rtl;
  }
&lt;/style&gt;
&amp;#32;&lt;span class=&#34;cloaked-e-mail&#34; data-user=&#34;sanoj&#34; data-domain=&#34;ti.dvohreke&#34; id=&#34;span-71e91440&#34;&gt;&lt;/span&gt;&amp;#32;

