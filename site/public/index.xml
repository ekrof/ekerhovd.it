  var link = document.createElement(&#34;a&#34;);
  var address = &#34;sanoj&#34;.split(&#39;&#39;).reverse().join(&#39;&#39;) + &#34;@&#34; + &#34;ti.dvohreke&#34;.split(&#39;&#39;).reverse().join(&#39;&#39;);
  link.href = &#34;mailto&#34; + &#34;:&#34; + address;
  
